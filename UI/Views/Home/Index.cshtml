                                                @for (var i = 1; i < Decimal.Round(item.MotherRate, 1); i++)
                                                {
                                                    <i class="fa fa-star" style="color: #D10024"></i>
                                                }
                                                @for (var i = Decimal.Round(item.MotherRate, 1); i <= 5; i++)
                                                {
                                                    @if (Decimal.Round(item.MotherRate, 1) != 0)
                                                    {
                                                        @if (Math.Floor((i - Math.Floor(i)) * 10) == 0 && i == Decimal.Round(item.MotherRate, 1))
                                                        {
                                                            <i class="fa fa-star" style="color: #D10024"></i>
                                                        }
                                                        else if (Math.Floor((i - Math.Floor(i)) * 10) >= 5 && i == Decimal.Round(item.MotherRate, 1))
