                                <div class="section-title" style="float:right">
                                    <h4 class="title"><a href="/Home/PowerSuply"><strong>show more</strong> <i class="fa fa-hand-o-right" aria-hidden="true"></i></a></h4>
                                </div>
                            </div>
                        </div>
                        <!-- /Toppowersupply-->
                        <!-- Topcase-->
                        <div id="tab17" class="tab-pane">
                            @foreach (var item in TopCases)
                            {
                                <div class="col-md-3 col-xs-6">
                                    <div class="product">
                                        <div class="product-img">
                                            @foreach (var img in item.Image)
                                            {
                                                <img src="~/Images/@img" alt="">
                                            }
                                        </div>
                                        <div class="product-body">
                                            <h3 class="product-name text-truncate" data-toggle="tooltip" title="@item.CaseName"><a href="javascript:gotoDetails('@item.CaseCode')">@item.CaseName</a></h3>
                                            <h4 class="product-price">@item.CasePrice <span>LE</span></h4>
                                            @if (item.CaseQuantity == 0)
                                            {
                                                <h3 class="product-price" style="font-size: 14px;">Out of Stock </h3>
                                            }
                                            else
                                            {
                                                <h3 class="product-price" style="font-size: 14px;color:green">In Stock </h3>
                                            }
                                            <div class="product-rating">
                                                @for (var i = 1; i < Decimal.Round(item.CaseRate, 1); i++)
                                                {
                                                    <i class="fa fa-star" style="color: #D10024"></i>
                                                }
                                                @for (var i = Decimal.Round(item.CaseRate, 1); i <= 5; i++)
                                                {
                                                    @if (Decimal.Round(item.CaseRate, 1) != 0)
                                                    {
                                                        @if (Math.Floor((i - Math.Floor(i)) * 10) == 0 && i == Decimal.Round(item.CaseRate, 1))
                                                        {
                                                            <i class="fa fa-star" style="color: #D10024"></i>
                                                        }
                                                        else if (Math.Floor((i - Math.Floor(i)) * 10) >= 5 && i == Decimal.Round(item.CaseRate, 1))
                                                        {
                                                            <i class="fa fa-star-half-o" style="color: #D10024"></i>
                                                        }
                                                        else
                                                        {
                                                            <i class="fa fa-star-o" style="color: #D10024"></i>
                                                        }
                                                    }
                                                    else
                                                    {
                                                        if (i < 5)
                                                        {
                                                            <i class="fa fa-star-o" style="color: #D10024"></i>
                                                        }
                                                    }
                                                }
                                            </div>
                                            <div class="product-btns">
                                                @if (item.WishList == true)
                                                {
                                                    <button onclick="AddOrDeleteWLIndex('@item.CaseCode','fromtop')" class="add-to-wishlist"><i id="t_@item.CaseCode" style="color: #D10024" class="fa fa-heart"></i><span class="tooltipp">Remove from wishlist</span></button>
                                                }
                                                else
                                                {
                                                    <button onclick="AddOrDeleteWLIndex('@item.CaseCode','fromtop')" class="add-to-wishlist"><i id="t_@item.CaseCode" class="fa fa-heart-o"></i><span class="tooltipp">add to wishlist</span></button>
                                                }
                                                <button onclick="gotoDetails('@item.CaseCode')" class="quick-view"><i class="fa fa-eye"></i><span class="tooltipp">quick view</span></button>
                                                @if (item.CaseQuantity == 0)
                                                {
                                                    <button style="background: white; cursor: auto" data-toggle="blog-tags" data-placement="top">
                                                        <i class="fa fa-shopping-cart" style="color: #cdcdcd; "></i>
                                                    </button>
                                                }
                                                else
                                                {
                                                    <button onclick="AddToCart({Code: '@item.CaseCode',Quantity: 1})" data-toggle="blog-tags" data-placement="top" title="Add TO CART">
                                                        <i class="fa fa-shopping-cart"></i><span class="tooltipp">add to Cart</span>
                                                    </button>
                                                }
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            }
                            <div class="col-md-12">
                                <div class="section-title" style="float:right">
                                    <h4 class="title"><a href="/Home/Case"><strong>show more</strong> <i class="fa fa-hand-o-right" aria-hidden="true"></i></a></h4>
                                </div>
                            </div>
                        </div>
                        <!-- /Topcase-->
                    </div>
                </div>
            </div>
            <!-- /Products tab & slick -->
        </div>
        <!-- /row -->
    </div>
    <!-- /container -->
</div>
<!-- /SECTION -->
<script src="~/js/jquery.min.js"></script>
<script>
    "use strict";

    $(document).ready(function () {
        $('#atag').hover(function () {
            $(this).css('color', '#aaa');
        });
        $('#atag').mouseout(function () {
            $(this).css('color', '#D10024');
        });

        $('[data-toggle="tooltip"]').tooltip();

    });

    function AddOrDeleteWLIndex(Product, txt) {
        if (($('#' + Product).hasClass("fa fa-heart") && txt == '') || ($('#t_' + Product).hasClass("fa fa-heart") && txt == 'fromtop')) {
            //لو ملونه هيروح بالاجكس على اكشن للديليت
            $.ajax({
                type: "POST",
                url: "/Home/DeleteFromWL?ProductCode=" + Product,
                dataType: "json",
                success: function (result) {
                    if (result == "Deleted Done") {
                        if (txt == 'fromtop') {
                            $('#t_' + Product).removeClass('fa fa-heart');
                            $('#t_' + Product).addClass("fa fa-heart-o").css('color', '');
                        }
                        else {
                            $('#' + Product).removeClass('fa fa-heart');
                            $('#' + Product).addClass("fa fa-heart-o").css('color', '');
                        }
                        toastr.success('Deleted Done.', '', { timeOut: 7000 });
                        wishlistconnection();
                    }
                    else if (result == "Error") {
                        toastr.error('Something Wrong to delete , Try Again.', '', { timeOut: 7000 });
                    }

                }
            });
        }
        else {
            $.ajax({
                //لو مش ملونه
                url: "/Home/AddToWL?ProductCode=" + Product,
                type: "POST",
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                success: function (result) {
                    if (result == 'Saved Done') {
                        if (txt == 'fromtop') {
                            $('#t_' + Product).removeClass('fa fa-heart-o');
                            $('#t_' + Product).addClass("fa fa-heart").css('color', '#D10024');
                        }
                        else {
                            $('#' + Product).removeClass('fa fa-heart-o');
                            $('#' + Product).addClass("fa fa-heart").css('color', '#D10024');
                        }
                        toastr.success('Product successfully added to your wishlist', '', { timeOut: 7000 });
                        wishlistconnection();
                    }
                    else if (result == 'LoginRegisterPopup') {
                        $('#login-register').click();
                    }
                    else {
                        toastr.error('Something Wrong to add , Try again!!', '', { timeOut: 7000 });
                    }

                }
            });
        }
    }

</script>